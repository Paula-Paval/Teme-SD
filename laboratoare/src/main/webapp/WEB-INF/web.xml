<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Hello</servlet-name>
    <servlet-class>HelloServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProcessStudent</servlet-name>
    <servlet-class>ProcessStudentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ReadStudent</servlet-name>
    <servlet-class>ReadStudentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateStudent</servlet-name>
    <servlet-class>UpdateStudentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteStudent</servlet-name>
    <servlet-class>DeleteStudentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ReadStudentById</servlet-name>
    <servlet-class>ReadStudentByIdServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ReadStudentByNume</servlet-name>
    <servlet-class>ReadStudentByNumeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ExportJSON</servlet-name>
    <servlet-class>ExportJSONServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessStudent</servlet-name>
    <url-pattern>/process-student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReadStudent</servlet-name>
    <url-pattern>/read-student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateStudent</servlet-name>
    <url-pattern>/update-student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteStudent</servlet-name>
    <url-pattern>/delete-student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReadStudentById</servlet-name>
    <url-pattern>/read-by-id</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReadStudentByNume</servlet-name>
    <url-pattern>/read-by-nume</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ExportJSON</servlet-name>
    <url-pattern>/export</url-pattern>
  </servlet-mapping>
</web-app>
